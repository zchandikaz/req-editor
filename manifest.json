{
  "name": "Req-Editor",
  "description": "Request Editor",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/res/img/logo-16_x_16.png",
      "32": "/res/img/logo-32_x_32.png",
      "48": "/res/img/logo-48_x_48.png",
      "128": "/res/img/logo-128_x_128.png"
    }
  },
  "icons": {
    "16": "/res/img/logo-16_x_16.png",
    "32": "/res/img/logo-32_x_32.png",
    "48": "/res/img/logo-48_x_48.png",
    "128": "/res/img/logo-128_x_128.png"
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ]
}